"LightmappedGeneric"
{
	"$basetexture" "lich/strips/strips7"
	"$bumpmap" "lich/strips/strips7_normal"
	"$surfaceprop" "Metal"
	"$selfillum" 1
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" "1"
	"$envmaptint"		"[0.1 0.1 0.1]"
Proxies
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "15"
      }
   }

Sine
{
resultVar $selfIllumTint
sinemin 0.70
sinemax 2.00
sineperiod 3.00
}
}