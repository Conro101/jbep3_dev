"VertexlitGeneric"
{
	"$basetexture" "models/weapons/v_ricochet/ricochet_metal"
	$envmap "env_cubemap"
	"$envmaptint" "[.04 .04 .04]"
	
	$bumpmap "models/weapons/v_ricochet/ricochet_normal"
	
	$selfillum 1
	$selfillummask "models/weapons/v_ricochet/ricochet_selfillum"
	
	$phong 1
	$phongexponent 40
	$phongboost 0.5
	$phongfresnelranges "[1.0 2.0 5.0]"
		
	// Blood effects
	$detail "models/blood.vtf"
	$detailscale "1.8"
	$detailblendfactor .001
	$detailblendmode 0
	$detailAmount 0
	Proxies
	{
		WeaponBlood
		{
			resultVar $detailAmount
		}
		Equals
		{
			srcVar1 $detailAmount
			resultVar $detailblendfactor			
		}
	}
}
