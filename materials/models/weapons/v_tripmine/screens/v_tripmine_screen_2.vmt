"VertexlitGeneric"
{
	"$basetexture" "models/weapons/v_tripmine/screens/v_tripmine_screen_2"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "5"
		}
		
		WeaponBlood
		{
			resultVar $detailAmount
		}
		Equals
		{
			srcVar1 $detailAmount
			resultVar $detailblendfactor			
		}
	}
	
	// Blood effects
	$detail "models/blood.vtf"
	$detailscale "1.8"
	$detailblendfactor .001
	$detailblendmode 0
	$detailAmount 0
}
