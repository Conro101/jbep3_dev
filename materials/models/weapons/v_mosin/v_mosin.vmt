{
	"$basetexture" "models/weapons/v_mosin/v_mosin"
	"$bumpmap" "models/weapons/v_mosin/v_mosin_normal"
	"$phong" "1"
	"$phongboost" "1"
	"$phongexponent" 10
	"$BasemapAlphaPhongMask" "1"
	"$normalmapalphaenvmapmask" "7"
